<?xml version="1.0"?>
<extension key="de.systopia.defaulteventmessages" type="module">
  <file>defaulteventmessages</file>
  <name>Default Event Messages</name>
  <description>This is an ad-on Extension for [de.systopia.eventmessages](https://github.com/systopia/de.systopia.eventmessages). It lets you configure a default Event Template on the settings page and if activated automatically copies the eventmessage config to the new event.

    This way if a lot of similar Events are created and the normal Event templates aren't used eventmessages is active by default.
  </description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Philipp Batroff</author>
    <email>batroff@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.defaulteventmessages</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.defaulteventmessages</url>
    <url desc="Support">https://github.com/systopia/de.systopia.defaulteventmessages/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-08-24</releaseDate>
  <version>0.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Defaulteventmessages</namespace>
    <format>22.05.2</format>
    <angularModule>crmDefaulteventmessages</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
